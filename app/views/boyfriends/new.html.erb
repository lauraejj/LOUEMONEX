<%= form_for(@boyfriend) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <%= f.label :ville %>
  <%= f.text_field :ville %>
   <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.file_field :photos, as: :file %>
  <%= f.submit %>
<% end %>
